{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rmst-tauri-test",
  "version": "0.1.0",
  "identifier": "com.rmst-tauri-test.app",
  "build": {
    "beforeDevCommand": "pnpm dev-web",
    "devUrl": "http://localhost:1422",
    "beforeBuildCommand": "pnpm build-web",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "rmst-tauri-test",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  },
  "plugins": {
    "updater": {
      "pubkey": "CONTENT FROM PUBLICKEY.PEM",
      "endpoints": ["https://github.com/rmstsd/rmst-tauri-test/releases/latest/download/latest.json"],
      "windows": {
        "installMode": "basicUi"
      }
    }
  }
}
